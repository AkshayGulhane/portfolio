    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.12.1",
    "stylelint": "^9.2.0",
