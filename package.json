    "intersection-observer": "^0.5.0",
    "js-yaml": "^3.12.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.2",
    "react-transition-group": "^2.3.1",
    "vcf": "^2.0.1"
